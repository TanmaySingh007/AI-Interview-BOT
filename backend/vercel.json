{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "OPENAI_MODEL": "gpt-4",
    "WHISPER_MODEL": "whisper-1"
  },
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}
